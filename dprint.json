{
    "typescript": {
        "indentWidth": 4,
        "quoteStyle": "preferSingle",
        "newLineKind": "auto"
    },
    "json": {
        "indentWidth": 4,
        "newLineKind": "auto"
    },
    "markdown": {
        "newLineKind": "auto"
    },
    "includes": [
        "**/*.{ts,tsx,js,jsx,cjs,mjs,json,md}"
    ],
    "excludes": [
        "**/node_modules",
        "**/*-lock.json",
        "**/dist",
        "**/VROOT"
    ],
    "plugins": [
        "https://plugins.dprint.dev/typescript-0.84.0.wasm",
        "https://plugins.dprint.dev/json-0.17.0.wasm",
        "https://plugins.dprint.dev/markdown-0.15.2.wasm"
    ]
}
